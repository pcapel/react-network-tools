{
  "name": "react-network-tools",
  "version": "1.0.1",
  "description": "Declarative data providers using socket.io and axios for react applications",
  "main": "dist/index.js",
  "repository": "https://github.rackspace.com/phil9824/react-network-tools.git",
  "author": "Philip Capel <Philip.Capel@rackspace.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test:watch": "jest --watch --env=jsdom",
    "test": "jest --env=jsdom",
    "build": "cross-env NODE_ENV=production babel --plugins transform-es2015-modules-commonjs src -d dist"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.5",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-watch": "^4.0.1",
    "mock-socket": "^8.0.1",
    "prop-types": "^15.6.2",
    "react-testing-library": "^4.1.2"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "axios": "^0.18.0",
    "jest": "22.4.4",
    "lodash": "^4.17.10",
    "object-assign": "4.1.1",
    "react": "^16.4.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
